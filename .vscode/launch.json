{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Generate (dev)",
            "type": "debugpy",
            "request": "launch",
            "program": "svg/app.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "${relativeFile}"
            ],
            "env": {
                "SVG_HOME": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Generate (local)",
            "type": "debugpy",
            "request": "launch",
            "program": "svg/app.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "${relativeFile}"
            ],
            "env": {
            }
        },
        {
            "name": "Python: Tests (file)",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "${file}"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Python: Run App",
            "type": "debugpy",
            "request": "launch",
            "program": "svg/app.py",
            "console": "integratedTerminal",
            "args": [
            ],
            "env": {
            }
        }
    ]
}